{% extends 'CatalogCommonBundle:Catalog:05_subgroups.html.twig' %}
{% use 'CatalogSubaruBundle::subaru.base.html.twig' %}
{% block breadcrumbs %}
    <a href="{{ path('catalog_subaru') }}">Выбрать регион, модель, модификацию</a> / <a href="{{ path('catalog_subaru_complectations', parameters) }}">Выбрать комплектацию</a> / <a href="{{ path('catalog_subaru_groups', parameters) }}">Выбрать группу запчастей</a> / Выбрать подгруппу запчастей
{% endblock breadcrumbs %}


{% block subgroupImage %}
        <div class="col-lg-8">
            <img src="{{ asset('bundles/catalogsubaru/Subaru_Images/'~oContainer.getActiveSchema.getOption(constant('Catalog\\CommonBundle\\Components\\Constants::CD'))~'/'~oContainer.getActiveSchema.getOption('sub_dir')~'/'~oContainer.getActiveSchema.getOption('sub_wheel')~'/model'~oContainer.getActiveSchema.getOption('num_model')~'/part1/'~oContainer.getActiveSchema.getOption('num_image')~'.png') }}" alt="{{ oContainer.getActiveGroup.getRuname }}" width="900" usemap="#{{ oContainer.getActiveGroup.getCode }}" />
        </div>
        {% endblock subgroupImage %}



{% block pathToSchemas %}
    {{ path('catalog_subaru_schemas', parameters|merge({'subGroupCode': subgroup.getCode})) }}
{% endblock pathToSchemas %}