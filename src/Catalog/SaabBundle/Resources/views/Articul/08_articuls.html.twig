{% extends 'CatalogSaabBundle:Catalog:08_articuls.html.twig' %}
{% block commonArticulPath %}{{ parameters.redirectAdress|replace({ '{article}': articul.getCode }) }}{% endblock commonArticulPath %}
{% block articulItem %}
    {% if articul.getOption('articul') == parameters.articul %}
        <b style=" color: red; ">{{ articul.getOption('articul') }}</b>
    {% else %}
        {{ articul.getOption('articul') }}
    {% endif %}
{% endblock articulItem %}