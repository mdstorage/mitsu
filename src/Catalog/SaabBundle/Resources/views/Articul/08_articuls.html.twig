{% extends 'CatalogSaabBundle:Catalog:08_articuls.html.twig' %}
{% block commonArticulPath %}{{ constant('Catalog\\CommonBundle\\Components\\Constants::FIND_PATH') }}{{ articul.getCode }}{% endblock commonArticulPath %}
{% block articulItem %}
    {% if articul.getOption('articul') == app.request.cookies.get(constant('Catalog\\CommonBundle\\Components\\Constants::ARTICUL')) %}
        <b style=" color: red; ">{{ articul.getOption('articul') }}</b>
    {% else %}
        {{ articul.getOption('articul') }}
    {% endif %}
{% endblock articulItem %}