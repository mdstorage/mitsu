<div class="modifications">
<div class="row">
    {% for modification in oContainer.getActiveModel.getModifications %}
         <div>
            {% block modification %}

                <a href="{{ path('catalog_saab_groups', parameters|merge({'modificationCode': modification.getCode, 'token': app.request.get('token')})) }}">

                    {{ modification.getRuname }}
                </a>

            {% endblock modification %}
        </div>
    {% endfor %}
</div>
</div>
