{% extends 'CatalogKiaBundle:Catalog:08_articuls.html.twig' %}
{% block commonArticulPath %}{{ parameters.redirectAdress|replace({ '{article}': articul.getCode }) }}{% endblock commonArticulPath %}
{% block articulItem %}
    {% if articul.getCode == parameters.articul %}
        <b style=" color: red; ">{{ articul.getCode }}</b>
    {% else %}
        {{ articul.getCode }}
    {% endif %}
{% endblock articulItem %}