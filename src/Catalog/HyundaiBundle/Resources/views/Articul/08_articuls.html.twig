{% extends 'CatalogHyundaiBundle:Catalog:08_articuls.html.twig' %}
{% block commonArticulPath %}{{ parameters.redirectAdress|replace({ '{article}': articul.getRuname }) }}{% endblock commonArticulPath %}
{% block articulItem %}
    {% if articul.getRuname == parameters.articul %}
        <b style=" color: red; ">{{ articul.getRuname }}</b>
    {% else %}
        {{ articul.getRuname }}
    {% endif %}
{% endblock articulItem %}