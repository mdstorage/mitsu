{% extends 'CatalogCommonBundle:Catalog:08_articuls.html.twig' %}
{% block commonArticulPath %}{{ constant('Catalog\\CommonBundle\\Components\\Constants::FIND_PATH') }}{{ articul.getCode }}{% endblock commonArticulPath %}
{% block articulItem %}
    {% if articul.getCode|replace({' ': ''}) == app.request.cookies.get(constant('Catalog\\CommonBundle\\Components\\Constants::ARTICUL'))|replace({' ': ''}) %}
        <b style=" color: red; ">{{ articul.getCode }}</b>
    {% else %}
        {{ articul.getCode }}
    {% endif %}
{% endblock articulItem %}