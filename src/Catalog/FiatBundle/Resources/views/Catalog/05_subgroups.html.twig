{% extends 'CatalogCommonBundle:Catalog:05_subgroups.html.twig' %}
{% use 'CatalogFiatBundle::fiat.base.html.twig' %}
{% block breadcrumbs %}
    <a href="{{ path('catalog_fiat') }}">Регион, модель, модификация ({{ oContainer.getActiveRegion.getRuname }},
        {{ oContainer.getActiveModel.getRuname }}, {{ oContainer.getActiveModification.getRuname }})</a> /
    <a href="{{ path('catalog_fiat_groups', parameters) }}">Группа запчастей ({{ oContainer.getActiveGroup.getCode}})</a> /
    Подгруппа запчастей
{% endblock breadcrumbs %}
{% block complectation %}{% endblock complectation %}


{% block subgroupImage %}{% endblock subgroupImage %}
{% block subgroupItem %}
    <div id="subgroup{{ subgroup.getCode }}">
        <a href="{{ path('catalog_fiat_schemas', parameters|merge({'subGroupCode': subgroup.getCode})) }}">({{ subgroup.getCode }}) {{ subgroup.getRuname }}</a>
    </div>
{% endblock subgroupItem %}